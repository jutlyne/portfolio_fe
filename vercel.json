{
    "headers": [
        {
            "source": "/sitemap-index.xml.gz",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "application/xml"
                },
                {
                    "key": "Content-Encoding",
                    "value": "gzip"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/sitemap-index.xml.gz",
            "destination": "/sitemap-index.xml.gz"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "develop": false
        }
    }
}